<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <appender name="stdout" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%d{HH:mm:ss.SSS} %5p [%30.30t] %-40.40c{1.}: %m%n</pattern>
    </encoder>
  </appender>
  <root level="WARN">
    <appender-ref ref="stdout"/>
  </root>
</configuration>